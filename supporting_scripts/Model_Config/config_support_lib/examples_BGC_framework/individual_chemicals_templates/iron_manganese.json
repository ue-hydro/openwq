{
  "_DESCRIPTION": "Iron and Manganese Redox Cycling",
  "_REFERENCE": "Stumm & Morgan (1996), WASP TOXI",
  "_VERSION": "1.0",

  "BIOGEOCHEMISTRY_CONFIGURATION": {
    "CHEMICAL_SPECIES": {
      "Fe_II": {
        "DESCRIPTION": "Ferrous iron (reduced, soluble)",
        "UNITS": "mg/L",
        "INITIAL_CONDITION": 0.5
      },
      "Fe_III": {
        "DESCRIPTION": "Ferric iron (oxidized, precipitates)",
        "UNITS": "mg/L",
        "INITIAL_CONDITION": 0.1
      },
      "Fe_oxy": {
        "DESCRIPTION": "Particulate iron oxyhydroxides (FeOOH)",
        "UNITS": "mg/L",
        "INITIAL_CONDITION": 1.0
      },
      "Mn_II": {
        "DESCRIPTION": "Manganous manganese (reduced, soluble)",
        "UNITS": "mg/L",
        "INITIAL_CONDITION": 0.2
      },
      "Mn_IV": {
        "DESCRIPTION": "Manganic manganese (oxidized, precipitates)",
        "UNITS": "mg/L",
        "INITIAL_CONDITION": 0.05
      },
      "MnO2": {
        "DESCRIPTION": "Particulate manganese dioxide",
        "UNITS": "mg/L",
        "INITIAL_CONDITION": 0.5
      },
      "DO": {
        "DESCRIPTION": "Dissolved oxygen",
        "UNITS": "mg/L",
        "INITIAL_CONDITION": 5.0
      },
      "H2S": {
        "DESCRIPTION": "Hydrogen sulfide",
        "UNITS": "mg/L",
        "INITIAL_CONDITION": 0.0
      },
      "PO4_P": {
        "DESCRIPTION": "Phosphate (Fe-P coupling)",
        "UNITS": "mg/L",
        "INITIAL_CONDITION": 0.1
      }
    },

    "CYCLING_FRAMEWORKS": {
      "IRON_REDOX": {
        "_DESCRIPTION": "Iron oxidation-reduction cycling",

        "FE_OXIDATION_ABIOTIC": {
          "CONSUMED": "Fe_II",
          "PRODUCED": "Fe_III",
          "KINETICS": "k_ox_Fe * Fe_II * DO * (OH)^2",
          "PARAMETERS": {
            "k_ox_Fe": {
              "VALUE": 8e12,
              "UNITS": "L3/mol2/atm/min",
              "DESCRIPTION": "Fe(II) oxidation rate constant"
            },
            "OH": {
              "VALUE": 1e-7,
              "UNITS": "mol/L",
              "DESCRIPTION": "Hydroxide concentration (pH dependent)"
            }
          },
          "NOTES": "Rate increases 100x per pH unit; very fast above pH 7"
        },

        "FE_OXIDATION_SIMPLIFIED": {
          "CONSUMED": "Fe_II",
          "PRODUCED": "Fe_III",
          "KINETICS": "k_ox_Fe_simple * Fe_II * (DO / (K_O2 + DO)) * 1.1^(T - 20)",
          "PARAMETERS": {
            "k_ox_Fe_simple": {
              "VALUE": 5.0,
              "UNITS": "1/day",
              "RANGE": [0.5, 50.0],
              "DESCRIPTION": "Simplified oxidation rate at neutral pH"
            },
            "K_O2": {"VALUE": 0.5, "UNITS": "mg/L"}
          }
        },

        "FE_III_PRECIPITATION": {
          "CONSUMED": "Fe_III",
          "PRODUCED": "Fe_oxy",
          "KINETICS": "k_precip_Fe * Fe_III",
          "PARAMETERS": {
            "k_precip_Fe": {
              "VALUE": 10.0,
              "UNITS": "1/day",
              "DESCRIPTION": "Fe(III) precipitation as oxyhydroxide"
            }
          },
          "NOTES": "Fe(III) rapidly precipitates above pH 3"
        },

        "FE_REDUCTION_MICROBIAL": {
          "CONSUMED": "Fe_oxy",
          "PRODUCED": "Fe_II",
          "KINETICS": "k_red_Fe * Fe_oxy * (K_O2_inhib / (K_O2_inhib + DO)) * 1.1^(T - 20)",
          "PARAMETERS": {
            "k_red_Fe": {
              "VALUE": 0.1,
              "UNITS": "1/day",
              "RANGE": [0.01, 1.0],
              "DESCRIPTION": "Microbial Fe reduction rate"
            },
            "K_O2_inhib": {
              "VALUE": 0.5,
              "UNITS": "mg/L",
              "DESCRIPTION": "O2 inhibition constant"
            }
          },
          "NOTES": "Dissimilatory iron reduction under anoxic conditions"
        },

        "FE_REDUCTION_SULFIDE": {
          "CONSUMED": "Fe_oxy",
          "PRODUCED": "Fe_II",
          "KINETICS": "k_FeS * Fe_oxy * H2S",
          "PARAMETERS": {
            "k_FeS": {
              "VALUE": 100.0,
              "UNITS": "L/mg/day",
              "DESCRIPTION": "Fe reduction by sulfide (fast)"
            }
          },
          "NOTES": "Chemical reduction by H2S very rapid"
        },

        "FE_OXY_SETTLING": {
          "CONSUMED": "Fe_oxy",
          "PRODUCED": "SED_Fe",
          "KINETICS": "v_s / depth * Fe_oxy",
          "PARAMETERS": {
            "v_s": {"VALUE": 2.0, "UNITS": "m/day"},
            "depth": {"VALUE": 5.0, "UNITS": "m"}
          }
        },

        "FE_SEDIMENT_FLUX": {
          "CONSUMED": "NONE",
          "PRODUCED": "Fe_II",
          "KINETICS": "J_Fe / depth * (K_O2_sed / (K_O2_sed + DO))",
          "PARAMETERS": {
            "J_Fe": {
              "VALUE": 20.0,
              "UNITS": "mg/m2/day",
              "RANGE": [5.0, 100.0],
              "DESCRIPTION": "Sediment Fe flux under anoxia"
            },
            "K_O2_sed": {"VALUE": 2.0, "UNITS": "mg/L"},
            "depth": {"VALUE": 5.0, "UNITS": "m"}
          }
        }
      },

      "MANGANESE_REDOX": {
        "_DESCRIPTION": "Manganese oxidation-reduction cycling",

        "MN_OXIDATION": {
          "CONSUMED": "Mn_II",
          "PRODUCED": "Mn_IV",
          "KINETICS": "k_ox_Mn * Mn_II * (DO / (K_O2 + DO)) * 1.08^(T - 20)",
          "PARAMETERS": {
            "k_ox_Mn": {
              "VALUE": 0.5,
              "UNITS": "1/day",
              "RANGE": [0.1, 5.0],
              "DESCRIPTION": "Mn(II) oxidation rate"
            },
            "K_O2": {"VALUE": 1.0, "UNITS": "mg/L"}
          },
          "NOTES": "Mn oxidation slower than Fe; microbially catalyzed"
        },

        "MN_PRECIPITATION": {
          "CONSUMED": "Mn_IV",
          "PRODUCED": "MnO2",
          "KINETICS": "k_precip_Mn * Mn_IV",
          "PARAMETERS": {
            "k_precip_Mn": {
              "VALUE": 5.0,
              "UNITS": "1/day",
              "DESCRIPTION": "MnO2 precipitation rate"
            }
          }
        },

        "MN_REDUCTION": {
          "CONSUMED": "MnO2",
          "PRODUCED": "Mn_II",
          "KINETICS": "k_red_Mn * MnO2 * (K_O2_inhib / (K_O2_inhib + DO)) * 1.1^(T - 20)",
          "PARAMETERS": {
            "k_red_Mn": {
              "VALUE": 0.2,
              "UNITS": "1/day",
              "RANGE": [0.05, 1.0],
              "DESCRIPTION": "Microbial Mn reduction rate"
            },
            "K_O2_inhib": {"VALUE": 0.5, "UNITS": "mg/L"}
          },
          "NOTES": "Mn reduction occurs at higher Eh than Fe reduction"
        },

        "MNO2_SETTLING": {
          "CONSUMED": "MnO2",
          "PRODUCED": "SED_Mn",
          "KINETICS": "v_s / depth * MnO2",
          "PARAMETERS": {
            "v_s": {"VALUE": 2.0, "UNITS": "m/day"},
            "depth": {"VALUE": 5.0, "UNITS": "m"}
          }
        },

        "MN_SEDIMENT_FLUX": {
          "CONSUMED": "NONE",
          "PRODUCED": "Mn_II",
          "KINETICS": "J_Mn / depth * (K_O2_sed / (K_O2_sed + DO))",
          "PARAMETERS": {
            "J_Mn": {
              "VALUE": 10.0,
              "UNITS": "mg/m2/day",
              "RANGE": [2.0, 50.0],
              "DESCRIPTION": "Sediment Mn flux under anoxia"
            },
            "K_O2_sed": {"VALUE": 2.0, "UNITS": "mg/L"},
            "depth": {"VALUE": 5.0, "UNITS": "m"}
          }
        }
      },

      "FE_P_COUPLING": {
        "_DESCRIPTION": "Iron-phosphorus interactions",

        "P_SORPTION_TO_FE": {
          "CONSUMED": "PO4_P",
          "PRODUCED": "Fe_P",
          "KINETICS": "k_ads_P_Fe * PO4_P * Fe_oxy",
          "PARAMETERS": {
            "k_ads_P_Fe": {
              "VALUE": 0.5,
              "UNITS": "L/mg/day",
              "DESCRIPTION": "P adsorption to Fe oxyhydroxides"
            }
          }
        },

        "P_RELEASE_FROM_FE": {
          "CONSUMED": "Fe_P",
          "PRODUCED": "PO4_P",
          "KINETICS": "k_des_P_Fe * Fe_P * (K_O2_inhib / (K_O2_inhib + DO))",
          "PARAMETERS": {
            "k_des_P_Fe": {
              "VALUE": 0.2,
              "UNITS": "1/day",
              "DESCRIPTION": "P release when Fe reduces"
            },
            "K_O2_inhib": {"VALUE": 0.5, "UNITS": "mg/L"}
          },
          "NOTES": "Classic internal P loading mechanism"
        }
      },

      "REDOX_SEQUENCE": {
        "_DESCRIPTION": "Terminal electron acceptor sequence",
        "_NOTES": "Under decreasing Eh (more reducing conditions)",

        "SEQUENCE": [
          {"TEA": "O2", "Eh_range": ">300 mV", "Process": "Aerobic respiration"},
          {"TEA": "NO3-", "Eh_range": "100-300 mV", "Process": "Denitrification"},
          {"TEA": "MnO2", "Eh_range": "0-200 mV", "Process": "Mn reduction"},
          {"TEA": "FeOOH", "Eh_range": "-100-100 mV", "Process": "Fe reduction"},
          {"TEA": "SO4--", "Eh_range": "-200-0 mV", "Process": "Sulfate reduction"},
          {"TEA": "CO2", "Eh_range": "<-200 mV", "Process": "Methanogenesis"}
        ]
      }
    },

    "COMPARTMENT_ASSIGNMENT": {
      "LAKE_HYPOLIMNION": ["IRON_REDOX", "MANGANESE_REDOX", "FE_P_COUPLING"],
      "RESERVOIR_BOTTOM": ["IRON_REDOX", "MANGANESE_REDOX", "FE_P_COUPLING"],
      "WETLAND": ["IRON_REDOX", "MANGANESE_REDOX", "FE_P_COUPLING"],
      "GROUNDWATER": ["IRON_REDOX", "MANGANESE_REDOX"]
    }
  }
}
