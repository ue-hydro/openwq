{
  "_DESCRIPTION": "Organic Carbon Cycle - DOC/POC dynamics with microbial processing",
  "_REFERENCE": "WASP8, Wetzel (2001) Limnology",
  "_VERSION": "1.0",

  "BIOGEOCHEMISTRY_CONFIGURATION": {
    "CHEMICAL_SPECIES": {
      "DOC_labile": {
        "DESCRIPTION": "Labile dissolved organic carbon",
        "UNITS": "mg/L as C",
        "INITIAL_CONDITION": 2.0
      },
      "DOC_refrac": {
        "DESCRIPTION": "Refractory dissolved organic carbon",
        "UNITS": "mg/L as C",
        "INITIAL_CONDITION": 5.0
      },
      "POC_labile": {
        "DESCRIPTION": "Labile particulate organic carbon",
        "UNITS": "mg/L as C",
        "INITIAL_CONDITION": 1.0
      },
      "POC_refrac": {
        "DESCRIPTION": "Refractory particulate organic carbon",
        "UNITS": "mg/L as C",
        "INITIAL_CONDITION": 2.0
      },
      "DIC": {
        "DESCRIPTION": "Dissolved inorganic carbon (CO2 + HCO3 + CO3)",
        "UNITS": "mg/L as C",
        "INITIAL_CONDITION": 20.0
      },
      "CH4": {
        "DESCRIPTION": "Methane (dissolved)",
        "UNITS": "mg/L as C",
        "INITIAL_CONDITION": 0.01
      },
      "DO": {
        "DESCRIPTION": "Dissolved oxygen",
        "UNITS": "mg/L",
        "INITIAL_CONDITION": 8.0
      },
      "ALGAE_C": {
        "DESCRIPTION": "Algal carbon biomass",
        "UNITS": "mg/L as C",
        "INITIAL_CONDITION": 0.5
      }
    },

    "CYCLING_FRAMEWORKS": {
      "CARBON_CYCLE": {
        "_DESCRIPTION": "Complete organic carbon cycling",

        "POC_LABILE_HYDROLYSIS": {
          "CONSUMED": "POC_labile",
          "PRODUCED": "DOC_labile",
          "KINETICS": "k_hydrol_L * POC_labile * 1.05^(T - 20)",
          "PARAMETERS": {
            "k_hydrol_L": {
              "VALUE": 0.15,
              "UNITS": "1/day",
              "RANGE": [0.05, 0.5],
              "DESCRIPTION": "Labile POC hydrolysis rate"
            }
          }
        },

        "POC_REFRAC_HYDROLYSIS": {
          "CONSUMED": "POC_refrac",
          "PRODUCED": "DOC_refrac",
          "KINETICS": "k_hydrol_R * POC_refrac * 1.03^(T - 20)",
          "PARAMETERS": {
            "k_hydrol_R": {
              "VALUE": 0.01,
              "UNITS": "1/day",
              "RANGE": [0.001, 0.05],
              "DESCRIPTION": "Refractory POC hydrolysis rate"
            }
          }
        },

        "DOC_LABILE_RESPIRATION": {
          "CONSUMED": "DOC_labile",
          "PRODUCED": "DIC",
          "KINETICS": "k_resp_L * DOC_labile * (DO / (K_O2 + DO)) * 1.07^(T - 20)",
          "PARAMETERS": {
            "k_resp_L": {
              "VALUE": 0.2,
              "UNITS": "1/day",
              "RANGE": [0.1, 0.5],
              "DESCRIPTION": "Labile DOC respiration rate"
            },
            "K_O2": {
              "VALUE": 0.5,
              "UNITS": "mg/L",
              "DESCRIPTION": "Half-saturation O2 for respiration"
            }
          }
        },

        "DOC_REFRAC_RESPIRATION": {
          "CONSUMED": "DOC_refrac",
          "PRODUCED": "DIC",
          "KINETICS": "k_resp_R * DOC_refrac * (DO / (K_O2 + DO)) * 1.05^(T - 20)",
          "PARAMETERS": {
            "k_resp_R": {
              "VALUE": 0.01,
              "UNITS": "1/day",
              "RANGE": [0.001, 0.05],
              "DESCRIPTION": "Refractory DOC respiration rate"
            },
            "K_O2": {"VALUE": 0.5, "UNITS": "mg/L"}
          }
        },

        "RESPIRATION_O2_CONSUMPTION": {
          "CONSUMED": "DO",
          "PRODUCED": "NONE",
          "KINETICS": "2.67 * (k_resp_L * DOC_labile + k_resp_R * DOC_refrac) * (DO / (K_O2 + DO)) * 1.07^(T - 20)",
          "PARAMETERS": {
            "k_resp_L": {"VALUE": 0.2, "UNITS": "1/day"},
            "k_resp_R": {"VALUE": 0.01, "UNITS": "1/day"},
            "K_O2": {"VALUE": 0.5, "UNITS": "mg/L"}
          },
          "NOTES": "Stoichiometry: 2.67 mg O2 per mg C respired (C6H12O6 + 6O2 -> 6CO2 + 6H2O)"
        },

        "METHANOGENESIS": {
          "CONSUMED": "DOC_labile",
          "PRODUCED": "CH4",
          "KINETICS": "k_meth * DOC_labile * (K_O2_inhib / (K_O2_inhib + DO)) * 1.10^(T - 20)",
          "PARAMETERS": {
            "k_meth": {
              "VALUE": 0.05,
              "UNITS": "1/day",
              "RANGE": [0.01, 0.2],
              "DESCRIPTION": "Methanogenesis rate"
            },
            "K_O2_inhib": {
              "VALUE": 0.2,
              "UNITS": "mg/L",
              "DESCRIPTION": "O2 inhibition for methanogenesis"
            }
          },
          "NOTES": "Strictly anaerobic process"
        },

        "METHANE_OXIDATION": {
          "CONSUMED": "CH4",
          "PRODUCED": "DIC",
          "KINETICS": "k_CH4_ox * CH4 * (DO / (K_O2_CH4 + DO)) * 1.08^(T - 20)",
          "PARAMETERS": {
            "k_CH4_ox": {
              "VALUE": 0.5,
              "UNITS": "1/day",
              "RANGE": [0.1, 2.0],
              "DESCRIPTION": "Methane oxidation rate"
            },
            "K_O2_CH4": {
              "VALUE": 0.5,
              "UNITS": "mg/L"
            }
          }
        },

        "METHANE_EBULLITION": {
          "CONSUMED": "CH4",
          "PRODUCED": "CH4_atm_loss",
          "KINETICS": "k_ebull * max(0, CH4 - CH4_sat)",
          "PARAMETERS": {
            "k_ebull": {
              "VALUE": 10.0,
              "UNITS": "1/day",
              "DESCRIPTION": "Ebullition rate when supersaturated"
            },
            "CH4_sat": {
              "VALUE": 0.02,
              "UNITS": "mg/L",
              "DESCRIPTION": "CH4 saturation concentration"
            }
          }
        },

        "PHOTOSYNTHESIS": {
          "CONSUMED": "DIC",
          "PRODUCED": "ALGAE_C",
          "KINETICS": "mu_max * ALGAE_C * (DIC / (K_DIC + DIC)) * f_light * f_nutrient * 1.066^(T - 20)",
          "PARAMETERS": {
            "mu_max": {
              "VALUE": 2.0,
              "UNITS": "1/day",
              "DESCRIPTION": "Maximum algal growth rate"
            },
            "K_DIC": {
              "VALUE": 1.0,
              "UNITS": "mg/L",
              "DESCRIPTION": "Half-saturation for DIC uptake"
            },
            "f_light": {"VALUE": 0.5, "DESCRIPTION": "Light limitation"},
            "f_nutrient": {"VALUE": 0.8, "DESCRIPTION": "Nutrient limitation"}
          }
        },

        "ALGAL_RESPIRATION": {
          "CONSUMED": "ALGAE_C",
          "PRODUCED": "DOC_labile",
          "KINETICS": "k_resp_A * ALGAE_C * 1.08^(T - 20)",
          "PARAMETERS": {
            "k_resp_A": {
              "VALUE": 0.1,
              "UNITS": "1/day",
              "DESCRIPTION": "Algal respiration rate"
            }
          }
        },

        "ALGAL_DEATH": {
          "CONSUMED": "ALGAE_C",
          "PRODUCED": "POC_labile",
          "KINETICS": "k_death * ALGAE_C",
          "PARAMETERS": {
            "k_death": {
              "VALUE": 0.05,
              "UNITS": "1/day",
              "DESCRIPTION": "Algal death rate"
            }
          }
        },

        "POC_SETTLING": {
          "CONSUMED": "POC_labile",
          "PRODUCED": "SED_C_loss",
          "KINETICS": "v_s / depth * POC_labile",
          "PARAMETERS": {
            "v_s": {
              "VALUE": 0.5,
              "UNITS": "m/day",
              "DESCRIPTION": "POC settling velocity"
            },
            "depth": {"VALUE": 2.0, "UNITS": "m"}
          }
        },

        "POC_REFRAC_SETTLING": {
          "CONSUMED": "POC_refrac",
          "PRODUCED": "SED_C_loss",
          "KINETICS": "v_s / depth * POC_refrac",
          "PARAMETERS": {
            "v_s": {"VALUE": 0.5, "UNITS": "m/day"},
            "depth": {"VALUE": 2.0, "UNITS": "m"}
          }
        },

        "CO2_EXCHANGE": {
          "CONSUMED": "DIC",
          "PRODUCED": "CO2_atm",
          "KINETICS": "k_CO2 * (DIC * f_CO2 - CO2_sat)",
          "PARAMETERS": {
            "k_CO2": {
              "VALUE": 0.5,
              "UNITS": "1/day",
              "DESCRIPTION": "CO2 gas exchange coefficient"
            },
            "f_CO2": {
              "VALUE": 0.01,
              "DESCRIPTION": "Fraction of DIC as free CO2"
            },
            "CO2_sat": {
              "VALUE": 0.5,
              "UNITS": "mg/L",
              "DESCRIPTION": "Atmospheric CO2 equilibrium"
            }
          },
          "NOTES": "Net flux can be positive (evasion) or negative (invasion)"
        }
      }
    },

    "COMPARTMENT_ASSIGNMENT": {
      "LAKE": ["CARBON_CYCLE"],
      "WETLAND": ["CARBON_CYCLE"],
      "RIVER": ["CARBON_CYCLE"]
    }
  }
}
